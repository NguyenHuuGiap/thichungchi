<!DOCTYPE html>
<html>
  <head>
    <title>GiaPha</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag "application", media: "all" %>
    <%= stylesheet_link_tag "styles", media: "all" %>
    <%= javascript_include_tag "application" %>
  </head>

  <body>
    <body>
      <div class="container" id="header">
        <%= render "home/menu" %>
      </div>
      <div class="container " id="content">
        <div class="row">
           <div class="col-md-3 no-left">
             <div class="new_left box">
               <%= render "home/menu_thu_vien_anh" %>
             </div>
             <div class="new_left box">
               <%= render "home/menu_noti_action" %>
             </div>
           </div>
           <%= yield %>
         </div>
      </div>
      <div class="container" id="footer">
        <%= render "layouts/footer" %>
      </div>
      <%= javascript_include_tag "application" %>
    </body>
  </body>
</html>
