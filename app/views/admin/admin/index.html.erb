<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Category</h1>
  </div>
  <div id="flash-message">
    <%= render "admin/shared/flash", flash: flash %>
  </div>
</div>
<div class="panel-body">
  <a href="<%= new_admin_admin_path %>" class="btn btn-success mg-bot-15" role="button">Create Admin</a>
  <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-allpage">
    <thead>
      <tr>
        <th>STT</th>
        <th>Email</th>
        <th>Created at</th>
        <th>Updated at</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody class="data-tbody">
      <% @admins.each_with_index do |admin, index| %>
        <%= render "datatable", admin: admin, index: index %>
      <% end %>
    </tbody>
  </table>
</div>
