<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Image</h1>
  </div>
  <div id="flash-message">
    <%= render "admin/shared/flash", flash: flash %>
  </div>
</div>
<div class="panel-body">
  <a href="<%= new_admin_upload_image_path %>" class="btn btn-success mg-bot-15" role="button">Add Image</a>
  <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-allpage">
    <thead>
      <tr>
        <th>STT</th>
        <th>Title</th>
        <th>Image</th>
        <th>Created at</th>
        <th>Updated at</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody class="data-tbody">
      <% @images.each_with_index do |image, index| %>
        <%= render "datatable", image: image, index: index %>
      <% end %>
    </tbody>
  </table>
</div>
