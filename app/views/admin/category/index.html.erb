<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Catgory</h1>
  </div>
  <div id="flash-message">
    <%= render "admin/shared/flash", flash: flash %>
  </div>
</div>
<div class="panel-body">
  <a href="<%= new_admin_category_path %>" class="btn btn-success mg-bot-15" role="button">Create Category</a>
  <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-allpage">
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Created at</th>
        <th>Updated at</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody class="data-tbody">
      <% @categories.each do |category| %>
        <%= render "datatable", category: category %>
      <% end %>
    </tbody>
  </table>
</div>
