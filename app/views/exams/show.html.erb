<body onselectstart="return false" oncontextmenu="return false">
  <div class="warning_error">
    <div class="content_warning" style="text-align: center;">
      <img style="width: 100%;" src="http://thichungchihanhnghe.com/404.png"> <br><br>
      <img width="100" src="http://thichungchihanhnghe.com/loading.gif">
    </div>
  </div>
  <form id="msform">
    <div style="display: block;width: 100%;display: block;position: relative;height: 50px;">
      <img style="width:40px;position: absolute;right: 205px;z-index: 2;top: -4px;border-radius: 50px;" src="http://thichungchihanhnghe.com/clock.jpg">
      <span id="future_date" style="color: red!important;width: 100%;background: white;width: 240px;height: 50px;line-height: 50px;font-size: 35px;color: blue;top: -8px;position: relative;z-index: 1;position: absolute;right: 0px;border-radius: 10px 10px 0px 0px;text-align: right;padding-right: 20px;" class="style colorDefinition size_lg">00 : 38 : 56</span>
    </div>
    <div id="title_tracnghiem" style="position: relative;z-index: 3;box-shadow: none;">
      <div class="left_tn">CM</div>
      <div class="right_tn">
        <div class="top_right_tn">
          <div style="width: 50%;float: left;">
            Định giá xây dựng - cấp mới
          </div>
          <div style="width: 48%;float: left;text-align: right;padding-right: 2%;">
            Thời gian thi (40 phút)
          </div>
        </div>
        <div class="bottom_right_tn">
          <div style="width: 48%;float: left;height: 30px;text-align: left;padding-left: 2%;">
            Thí sinh: Nguyễn Luyện
          </div>
          <div style="width: 47%;float: left;text-align: right;padding-left: 1%;">
            Môn Thi: Định giá xây dựng
          </div>
        </div>
      </div>
    </div>
    <!-- progressbar -->
    <%= render "progress_bar", question_length: @questions.length %>
    <% @questions.each_with_index do |question, index| %>
      <!-- fieldsets -->
      <% if index.zero? %>
        <%= render("question_number1", question: question, index: index) %>
      <% elsif index == (@questions.length - 1) %>
        <%= render "question_number_last", question: question, index: index %>
      <% else %>
        <%= render "question_number", question: question, index: index %>
      <% end %>
    <% end %>
  </form>
  <div>
    <div class="sweet-overlay" tabindex="-1"></div>
    <div class="sweet-alert" tabindex="-1">
      <div class="icon error"><span class="x-mark"><span class="line left"></span><span class="line right"></span></span></div>
      <div class="icon warning"> <span class="body"></span> <span class="dot"></span> </div>
      <div class="icon info"></div>
      <div class="icon success">
        <span class="line tip"></span> <span class="line long"></span>
        <div class="placeholder"></div>
        <div class="fix"></div>
      </div>
      <div class="icon custom"></div>
      <h2>Title</h2>
      <p>Text</p>
      <button class="cancel" tabindex="2">Cancel</button><button class="confirm" tabindex="1">OK</button>
    </div>
  </div>
</body>
